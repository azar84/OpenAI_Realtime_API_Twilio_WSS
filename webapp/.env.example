# Next.js Configuration
NEXT_PUBLIC_APP_NAME=OpenAI Realtime Twilio Demo

# Backend WebSocket Server URL (for development)
NEXT_PUBLIC_WS_URL=ws://localhost:8081

# OpenAI Configuration (if needed in frontend)
NEXT_PUBLIC_OPENAI_MODEL=gpt-4o-realtime-preview-2024-12-17

# Twilio Configuration (if needed in frontend)
NEXT_PUBLIC_TWILIO_ACCOUNT_SID=your-twilio-account-sid

# Development Settings
NODE_ENV=development
NEXT_PUBLIC_DEBUG=false

# Auto-generated by startup scripts (do not set manually)
NGROK_URL=https://your-ngrok-url.ngrok-free.app
NGROK_WS_URL=wss://your-ngrok-url.ngrok-free.app
NGROK_HTTPS_URL=https://your-ngrok-url.ngrok-free.app
